@{
    ViewData["Title"] = "Journal";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>

    .content-container {
        display: flex;
        margin-top: 20px;
        background-color: white;
        box-shadow: 0 0 10px rgba(0,0,0,0.1);
    }

    .sidebar {
        width: 30%;
        padding: 20px;
        background-color: #f9f9f9;
        border-right: 1px solid #ddd;
    }

    .sidebar-title {
        background-color: #eee;
        padding: 15px;
        text-align: center;
        font-weight: bold;
        margin-bottom: 20px;
        border-radius: 5px;
    }

    .sidebar-image {
        background-color: #3b3b64;
        height: 200px;
        margin-bottom: 20px;
        border-radius: 5px;
    }

    .sidebar-button {
        display: block;
        width: 100%;
        padding: 12px;
        margin-bottom: 15px;
        background-color: #8b0000;
        color: white;
        border: none;
        border-radius: 5px;
        font-weight: bold;
        text-align: center;
        cursor: pointer;
        text-decoration: none;
    }

        .sidebar-button:hover {
            background-color: #a52a2a;
        }

    .main-content {
        width: 70%;
        padding: 20px;
    }

    .journal-title {
        font-size: 14px;
        font-weight: bold;
       
        color: #8b0000;
        align-items:center;
        text-align:center;
    }

    .journal-details {
        display: grid;
        grid-template-columns: 150px 1fr;
        
        margin-bottom: 20px;
    }

    .detail-label {
        font-weight: bold;
    }

    .open-journal {
        text-align: center;
        margin-top: 30px;
    }

    .open-journal-button {
        padding: 12px 30px;
        background-color: #8b0000;
        color: white;
        border: none;
        border-radius: 5px;
        font-weight: bold;
        cursor: pointer;
    }

        .open-journal-button:hover {
            background-color: #a52a2a;
        }

    a {
        color: #0066cc;
        text-decoration: none;
    }

        a:hover {
            text-decoration: underline;
        }

    .sidebar-box123
     {
        background-color: white;
        padding: 10px;
        margin-bottom: 10px;
        border-radius: 8px;
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.08);
    }
    .font_css
    {
    font-size:14px;
    }
</style>

